{
  "extends": "airbnb",
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "html"
  ],
  "ignorePatterns": [
    "*.html"
  ],
  "rules": {
    "max-len": [
      2,
      100
    ],
    "@typescript-eslint/no-unused-vars": 2
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
}